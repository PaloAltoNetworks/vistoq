                  <profile-setting>
                    <group>
                      <member>gold-base</member>
                    </group>
                  </profile-setting>
                  <to>
                    <member>L3-untrust</member>
                  </to>
                  <from>
                    <member>L3-trust</member>
                  </from>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
